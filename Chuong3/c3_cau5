def test_case(i, j, k):
    if i<j:
        if j < k:
            i = j
        else:
            j = k
    else:
        if j > k:
            j = i
        else:
            i = k
    print("i = ", i,"j = ", i, "k = ", k)
case = {
    "a":(3, 5, 7),
    "b":(3, 7, 5),
    "c":(5, 3, 7),
    "d":(5, 7, 3),
    "e":(7, 3, 5),
    "f":(7, 5, 3),
}
for name, (i, j, k) in case.items():
    print(f"Case {name}: input (i={i}, j={j}, k={k}) -> ", end="")
    test_case(i, j ,k)